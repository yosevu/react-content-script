{
  "manifest_version": 3,
  "name": "React Content Script",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "popup.html",
    "default_icon": "logo192.png"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
